                            <li class="menu-item menu-item-type-taxonomy menu-item-object-category <#if menu.children?? && menu.children?size gt 0>menu-item-has-children</#if> menu-item-${menu.id}">
                                <a href="${menu.url!}">${menu.name!}</a>
                                <ul class="sub-menu">
                                    <#list menu.children as child>
                                        <li id="menu-item-${child.id}" class="menu-item menu-item-type-taxonomy menu-item-object-category menu-item-${child.id}">
                                            <a href="${child.url!}">${child.name!}</a>
                                        </li>
                                    </#list>
                                </ul>
                            </li>
                        <#else >
                            <li class="menu-item menu-item-type-taxonomy menu-item-object-category menu-item-${menu.id}">
                                <a href="${menu.url!}">${menu.name!}</a>
                            </li>
                        </#if>
                    </#list>
                <#--</@menuTag>-->
            </ul>
        </div>
    </div>
</aside>
