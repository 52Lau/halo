        </form>
    </div>
    <div class="search-key-push px-3 px-lg-5 py-3 py-lg-4">
        <div class="h6 mb-3">
            <i class="text-danger text-xl iconfont icon-remen mr-1"></i>
            <span class="align-middle">热门搜索</span>
        </div>
        <ul class="nav nav-pills m-n1">
            <#--标签开始-->
            <#--<@tagTag method="list">-->
                <#list tags as tag>
                    <li class="nav-item p-1">
                        <a href="${context!}/tags/${tag.slugName!}" target="_blank" class="btn btn-light btn-sm">${tag.name}</a>
                    </li>
                </#list>
            <#--</@tagTag>-->
            <#--标签开始-->
        </ul>
    </div>
</div>
